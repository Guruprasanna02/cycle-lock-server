// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model CycleStand {
  id           String       @id @unique
  location     String       @db.VarChar(255)
  cycleDock    CycleDock[]
}

model CycleDock {
  id             String         @id @unique
  cycleStand     CycleStand     @relation(fields: [cycleStandId], references: [id])
  cycleStandId   String
}